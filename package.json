{
  "private": "true",
  "license": "MIT",
  "scripts": {
    "lint": "$(npm bin)/eslint src/**/*.{js,jsx} --quiet",
    "coverage": "$(npm bin)/babel-node $(npm bin)/isparta cover node_modules/mocha/bin/_mocha -- `scripts/get-test-directories.sh`",
    "coverage-token": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "test-only": "npm run lint && $(npm bin)/babel-node $(npm bin)/_mocha `./scripts/get-test-directories.sh`",
    "test": "npm run lint && npm run coverage && npm run coverage-token"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-enzyme": "^0.4.0",
    "codecov.io": "^0.1.6",
    "enzyme": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.0.2",
    "eslint-plugin-react": "^3.14.0",
    "isparta": "^4.0.0",
    "lerna": "^1.0.5",
    "mocha": "^2.3.4",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "sinon": "^1.17.3"
  }
}
