{
  "private": "true",
  "license": "MIT",
  "scripts": {
    "lint": "$(npm bin)/eslint src/**/*.{js,jsx} --quiet || true",
    "coverage": "BABEL_ENV=cli $(npm bin)/babel-node $(npm bin)/babel-istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- `scripts/get-test-directories.sh`",
    "coverage-token": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "demo": "BABEL_ENV=webpack scripts/bundle-demos.sh",
    "test": "npm run lint && BABEL_ENV=cli $(npm bin)/babel-node $(npm bin)/_mocha `./scripts/get-test-directories.sh`",
    "everything": "npm run lint && npm run coverage && npm run coverage-token"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-istanbul": "^0.6.1",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-webpack": "^6.3.14",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.4.1",
    "chai-enzyme": "^0.4.0",
    "codecov.io": "^0.1.6",
    "enzyme": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^4.0.0",
    "jsdom": "^8.1.0",
    "mocha": "^2.3.4",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "sinon": "^1.17.3",
    "webpack": "^2.1.0-beta.2"
  },
  "dependencies": {
    "cuid": "^1.3.8",
    "d3-axis": "^0.3.0",
    "d3-scale": "^0.6.3",
    "d3-selection": "^0.7.0",
    "d3-shape": "^0.5.1",
    "lodash": "^4.5.0",
    "random-js": "^1.0.8",
    "react": "^0.14.7",
    "react-faux-dom": "^2.4.0"
  }
}
